<script>
$(function(){
    var diaryModalSwiper = new Swiper('.bnf_swiper', {
        loop:true,
        navigation: {
            nextEl: '.btn_next',
            prevEl: '.btn_prev',
        },
        autoplay:true,
        observer: true,	
        observeParents: true,
    });
    var diaryModalSwiper02 = new Swiper('.bnf_swiper_02', {
        loop:true,
        navigation: {
            nextEl: '.btn_next',
            prevEl: '.btn_prev',
        },
        autoplay:true,
        observer: true,	
        observeParents: true,
    });
    // 모바일에서는 화면을 움직이면 자동 슬라이드 기능 재생.
    document.addEventListener('touchmove', () => {
    console.log('touchmove, startAutoplay')
    diaryModalSwiper.autoplay.start()
    })
    document.addEventListener('touchmove', () => {
    console.log('touchmove, startAutoplay')
    diaryModalSwiper02.autoplay.start()
    });
});

// 모달 오픈
function fnOpenBenefitModal() {
    //toggleScrolling();
    <% if isapp = 1 then %>
		setTimeout(function(){
			fnSetGnbTabbarDim(true);
            $('.modalV20').addClass('show');
		},500);
    <% else %>
        $('.modalV20').addClass('show');
    <% end if %>
}

// 모달 닫기
function fnCloseBenefitModal() {
    //toggleScrolling();
    <% if isapp = 1 then %>
		setTimeout(function(){
			fnSetGnbTabbarDim(false);
            $('.modalV20').removeClass('show');
		},500);
    <% else %>
        $('.modalV20').removeClass('show');
    <% end if %>
}

// 모달 호출될 때, 부모창 스크롤 방지
function toggleScrolling() {
    if ($('#benefitInfo').is(':visible')) {
		currentY = $(window).scrollTop();
        $('html').addClass('not_scroll');
    } else {
        $('html').removeClass('not_scroll');
        $('html').animate({scrollTop : currentY}, 0);
    }
}
</script>
